  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.2</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Orgdedupe</namespace>
  </civix>
</extension>

