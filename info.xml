<?xml version="1.0"?>
<extension key="uk.me.davidknoll.civicrm.orgdedupe" type="module">
  <file>orgdedupe</file>
  <name>Organisation Name De-duplicator</name>
  <description>Lists pairs of similar organisation names (differing by apostrophes etc) and allows the user to merge the records.</description>
  <urls>
    <url desc="Documentation">https://github.com/futurefirst/orgdedupe/wiki</url>
    <url desc="On CiviCRM.org">https://civicrm.org/extensions/organisation-name-de-duplicator</url>
    <url desc="On GitHub">https://github.com/futurefirst/orgdedupe</url>
    <url desc="Future First">http://www.futurefirst.org.uk/</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>David Knoll</author>
    <email>david@futurefirst.org.uk</email>
  </maintainer>
  <releaseDate>2014-10-01</releaseDate>
  <version>1.2</version>
  <compatibility>
    <ver>4.2</ver>
    <ver>4.3</ver>
    <ver>4.4</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Orgdedupe</namespace>
  </civix>
</extension>
